FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoFoamFHD;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         5.0;

deltaT          0.0001;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

postProcess -func vorticity;


functions
{
    #includeFunc residuals(U)
    #includeFunc streamlinesLine(funcName=streamlines, start=(0 1 0), end=(0 0 0), nPoints=24, U)
}
// ************************************************************************* //
